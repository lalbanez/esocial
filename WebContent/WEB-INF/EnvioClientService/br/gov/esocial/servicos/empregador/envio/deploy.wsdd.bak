<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from EnvioClientService WSDL service -->

  <service name="EnvioClient" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://envio.empregador.servicos.esocial.gov.br"/>
      <parameter name="wsdlServiceElement" value="EnvioClientService"/>
      <parameter name="schemaQualified" value="http://envio.empregador.servicos.esocial.gov.br"/>
      <parameter name="wsdlServicePort" value="EnvioClient"/>
      <parameter name="className" value="br.gov.esocial.servicos.empregador.envio.EnvioClientSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="EnvioClient"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="envioEsocial" qname="operNS:envioEsocial" xmlns:operNS="http://envio.empregador.servicos.esocial.gov.br" returnQName="retNS:envioEsocialReturn" xmlns:retNS="http://envio.empregador.servicos.esocial.gov.br" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:xml" xmlns:pns="http://envio.empregador.servicos.esocial.gov.br" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="envioEsocial"/>

  </service>
</deployment>
